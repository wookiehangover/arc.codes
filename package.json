{
  "architect": {
    "app": "v8-arc-codes",
    "aws": {
      "region": "us-west-1",
      "profile": "openjsf",
      "bucket": "arc.codes-deploy",
      "apigateway": "http"
    },
    "static": {
      "folder": "public",
      "fingerprint": true
    },
    "http": [
      [
        "get",
        "/docs/:lang/*"
      ]
    ]
  },
  "scripts": {
    "deploy": "npx arc deploy",
    "lint": "eslint src --ignore-pattern node_modules --fix",
    "start": "sandbox",
    "test": "tape -r esm test/*.test.js test/**/*.js | tap-spec"
  },
  "dependencies": {
    "@architect/architect": "8.3.7",
    "esm": "^3.2.25",
    "slugify": "^1.4.6",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.1"
  }
}
